<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<settings>
		<setting name="jdbcTypeForNull" value="NULL"/>
	</settings>
	<typeAliases>
		<typeAlias type="com.semi.travelpalette.travel.domain.Travel" alias="Travel"/>
		<typeAlias type="com.semi.travelpalette.community.domain.Community" alias="Community"/>
		<typeAlias type="com.semi.travelpalette.event.domain.Event" alias="Event"/>
		<typeAlias type="com.semi.travelpalette.user.domain.User" alias="User"/>
		<typeAlias type="com.semi.travelpalette.travel.domain.TravelFile" alias="TravelFile"/>
		<typeAlias type="com.semi.travelpalette.travel.domain.Review" alias="Review"/>
		<typeAlias type="com.semi.travelpalette.community.domain.Reply" alias="Reply"/>
		<typeAlias type="com.semi.travelpalette.common.domain.Like" alias="Like"/>
		<typeAlias type="com.semi.travelpalette.inquiry.domain.Inquiry" alias="Inquiry"/>
		<typeAlias type="com.semi.travelpalette.user.domain.UserMypageDto" alias="userMypageDto"/>
		<typeAlias type="com.semi.travelpalette.admin.domain.Response" alias="Response"/>
	</typeAliases>
	
	<mappers>
		<mapper resource="mappers/travel-mapper.xml"/>
		<mapper resource="mappers/community-mapper.xml"/>
		<mapper resource="mappers/event-mapper.xml"/>
		<mapper resource="mappers/user-mapper.xml"/>
		<mapper resource="mappers/travelFile-mapper.xml"/>
		<mapper resource="mappers/review-mapper.xml"/>
		<mapper resource="mappers/reply-mapper.xml"/>
		<mapper resource="mappers/like-mapper.xml"/>
		<mapper resource="mappers/inquiry-mapper.xml"/>		
		<mapper resource="mappers/admin-mapper.xml"/>	
	</mappers>
